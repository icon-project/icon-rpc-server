{
	"log": {
		"logger": "iconrpcserver",
		"colorLog": true,
		"level": "info",
		"filePath": "./log/iconservice.log",
		"outputType": "console|file",
		"rotate": {
			"type": "period|bytes",
			"period": "daily",
			"interval": 1,
			"backupCount": 10,
			"maxBytes": 10485760
		}
	},
	"channel": "loopchain_default",
	"port": 9000,
	"amqpTarget": "127.0.0.1",
	"amqpKey": "7100",
	"gunicornWorkerCount": 1
}
